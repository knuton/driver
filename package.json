{
  "name": "dividat-driver",
  "productName": "Dividat Driver",
  "version": "0.3.5",
  "description": "Dividat drivers and hardware test suites",
  "main": "src/main.js",
  "scripts": {
    "start": "nodemon src/server.js",
    "electron": "electron .",
    "build": "yarn && npm run check-cert && rimraf build && mkdirp build && node bin/build.js && node bin/package.js",
    "release": "node bin/release.js",
    "replay": "nodemon --watch src-test/ src-test/",
    "check-cert": "node bin/check-cert.js ssl/cert.pem",
    "test": "mocha test/smoke.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dividat/driver.git"
  },
  "author": "Dividat AG",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dividat/driver/issues"
  },
  "homepage": "https://github.com/dividat/driver#readme",
  "dependencies": {
    "bonjour": "^3.5.0",
    "cors": "^2.8.1",
    "electron-config": "^0.2.1",
    "electron-log": "^1.2.2",
    "express": "^4.14.0",
    "fantasy-land": "^1.0.1",
    "multicast-dns": "git://github.com/bnielsen1965/multicast-dns",
    "ramda": "^0.23.0",
    "socket.io": "^1.7.3"
  },
  "devDependencies": {
    "aws-sdk": "^2.22.0",
    "binary-split": "^1.0.3",
    "electron": "^1.4.12",
    "electron-packager": "^8.7.0",
    "electron-winstaller": "^2.5.2",
    "glob": "^7.1.1",
    "jszip": "^3.1.3",
    "mime-types": "^2.1.14",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "prompt": "^1.0.0",
    "rcedit": "^0.7.0",
    "request": "^2.81.0",
    "rimraf": "^2.5.4",
    "semver": "^5.3.0",
    "signcode": "^1.0.0",
    "tmp": "^0.0.33"
  }
}
